# AWS Configuration
AWS_REGION=eu-west-1
AWS_PROFILE=default

# Stage
STAGE=dev

# JWT Secret (also set in SSM Parameter Store)
JWT_SECRET=your-super-secret-jwt-key-here

# OpenAI API Key (also set in SSM Parameter Store)
OPENAI_API_KEY=your-openai-api-key-here

# DynamoDB Tables (auto-generated by serverless)
USERS_TABLE=synthai-strategist-backend-dev-users
SOCIAL_ACCOUNTS_TABLE=synthai-strategist-backend-dev-social-accounts
POSTS_TABLE=synthai-strategist-backend-dev-posts
COMMENTS_TABLE=synthai-strategist-backend-dev-comments
DOCUMENTS_TABLE=synthai-strategist-backend-dev-documents

# S3 Bucket (auto-generated by serverless)
S3_BUCKET=synthai-strategist-backend-dev-storage

# Local development
IS_OFFLINE=true